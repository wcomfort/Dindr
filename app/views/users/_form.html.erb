<h1>Please input the fields to create an account</h1>
<%= form_for @user do |f| %>
    <%= f.label :first_name %>
    <%= f.text_field :first_name %> <br> <br>
    <%= f.label :last_name %>
    <%= f.text_field :last_name %><br> <br>
    <%= f.label :email %>
    <%= f.text_field :email %> <br> <br>
    <%= f.label :password %>
    <%= f.password_field :password %><br><br>
<%= f.submit "Create Account"%>
<%end%>